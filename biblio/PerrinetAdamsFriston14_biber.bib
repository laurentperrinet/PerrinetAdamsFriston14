  JOURNALTITLE         = {Experimental Brain Research},
  KEYWORDS             = {learning, learningdiscrimination, modelsmotion, muscles, perception, performance, performancepsychomotor, physiology, stimulationpsychomotor, testsoculomotor, time, timereaction},
