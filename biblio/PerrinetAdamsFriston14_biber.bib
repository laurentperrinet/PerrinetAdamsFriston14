  JOURNALTITLE         = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
  KEYWORDS             = {cytologyvisual, fascicularismacaca, methodspursuitreaction, pathways, pathwaysvisual, perception, physiology, stimulation, stimulationphotic, time, timereaction},
  MONTH                = {03},
  NUMBER               = {6},
  PAGES                = {2224--2246},
  POSTED-AT            = {2013-06-14 13:00:53},
