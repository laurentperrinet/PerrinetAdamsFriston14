  DOI                  = {10.1016/S0301-0082(97)00039-7},
  ISSN                 = {0301-0082},
  JOURNALTITLE         = {Progress in Neurobiology},
  KEYWORDS             = {kaplan13, khoei13jpp, spem, visual-tracking},
  MONTH                = {10},
  NUMBER               = {3},
  PAGES                = {293--329},
  POSTED-AT            = {2012-10-09 14:45:10},
  PRIORITY             = {2},
  TITLE                = {Slow eye movements},
