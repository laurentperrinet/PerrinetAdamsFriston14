  JOURNALTITLE         = {Journal of {P}hysiology ({P}aris)},
  KEYWORDS             = {bayesian, center-surround, eye, following, integration, interactions, model, motion, movements, ocular, perception, re-, sponse, tracking, visual},
  NUMBER               = {1--3},
  PAGES                = {46--55},
