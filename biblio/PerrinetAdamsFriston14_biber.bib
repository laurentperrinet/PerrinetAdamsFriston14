  CITEULIKE-LINKOUT-1  = {http://www.journalofvision.org/content/11/11/533.abstract},
  CITEULIKE-LINKOUT-2  = {http://www.journalofvision.org/content/11/11/533.full.pdf},
  DAY                  = {23},
  DOI                  = {10.1167/11.11.533},
